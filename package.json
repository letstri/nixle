{
  "name": "scalex",
  "version": "0.1.3",
  "description": "Universal server-side architectural framework",
  "scripts": {
    "prepublish": "npm run build",
    "dev": "vite",
    "build": "vite build && tsc"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/scalex.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/scalex.js",
      "require": "./dist/scalex.cjs"
    },
    "./nitro": "./src/providers/nitro.ts"
  },
  "keywords": [
    "scalex",
    "framework",
    "server",
    "universal",
    "typescript",
    "node",
    "fastify",
    "express",
    "bun",
    "elysia",
    "server-side"
  ],
  "devDependencies": {
    "@types/express": "^4.17.21",
    "elysia": "^0.7.29",
    "express": "^4.18.2",
    "fastify": "^4.24.3",
    "nitropack": "^2.8.0",
    "typescript": "^5.3.2",
    "vite": "^5.0.2"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "dayjs": "^1.11.10",
    "h3": "^1.9.0"
  },
  "engines": {
    "node": ">=18"
  },
  "license": "MIT",
  "homepage": "https://scalex.letstri.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/letstri/scalex"
  },
  "author": {
    "name": "Valerii Strilets",
    "email": "valerii.strilets@gmail.com"
  }
}
