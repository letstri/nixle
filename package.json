{
  "name": "scalex",
  "version": "1.0.0-alpha.2",
  "description": "Universal server-side architectural framework",
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/scalex.umd.cjs",
  "module": "./dist/scalex.js",
  "exports": {
    ".": {
      "import": "./dist/scalex.js",
      "require": "./dist/scalex.umd.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "keywords": [
    "scalex",
    "framework",
    "server",
    "universal",
    "typescript",
    "node",
    "fastify",
    "express",
    "bun",
    "elysia",
    "server-side"
  ],
  "devDependencies": {
    "@types/express": "^4.17.21",
    "elysia": "^0.7.29",
    "express": "^4.18.2",
    "fastify": "^4.24.3",
    "nitropack": "^2.8.0",
    "typescript": "^5.3.2",
    "vite": "^5.0.2"
  },
  "dependencies": {
    "@vercel/analytics": "^1.1.1",
    "h3": "^1.9.0"
  },
  "author": {
    "name": "Valerii Strilets",
    "email": "valerii.strilets@gmail.com"
  },
  "license": "MIT",
  "homepage": "https://scalex.letstri.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/letstri/scalex"
  }
}
