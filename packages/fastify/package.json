{
  "name": "@nixle/fastify",
  "version": "0.2.7",
  "description": "Universal server-side architectural framework",
  "scripts": {
    "prepublishOnly": "npm run build",
    "dev": "vite",
    "build": "vite build && tsc && tsconfig-replace-paths"
  },
  "type": "module",
  "files": [
    "dist"
  ],
